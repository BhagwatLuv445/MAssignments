
public class Recursion {
	public static int function(int num){
		if(num==0)
			return 1;
		return num*function(num-1);
	} 

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		/*for(int i=1;i<=100;i++){
			System.out.print(i+"\t");*/
		System.out.println(function(5));
		}		
	}


