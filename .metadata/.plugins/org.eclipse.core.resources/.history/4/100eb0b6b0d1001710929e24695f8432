package SortingMethods;

public class BubbleSort {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int arr[]=new int[]{1,4,8,5,3,9,7,4};
		for(int i=arr.length;i>=0;i--){
			for(int j=1;j<i-1;j++){
				if(arr[j]>arr[j+1]){
					int temp=arr[j+1];
					arr[j+1]=arr[j];
					arr[j]=temp;
				}
			}
		}
		for(int a:arr){
			System.out.println(a);
		}
	}

}
